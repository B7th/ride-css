@import 'ride-css/css/*'

.set-properties-single
  set-properties: top 0

.set-properties-multy
  set-properties: top 0 left 0

.set-properties-multy-absurd
  set-properties: top 0 left 0 padding 1em margin 2em

.absolute-one
  absolute: top 0

.absolute-full
  absolute: top 0 left 0

.fixed
  fixed: top 0 left 0

.relative
  relative: top 0 left 0

.sticky
  sticky: top 0 left 0
